<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<fragment>
    
    <extension
        point="org.eclipse.ui.views">
        <view
            name="%MarkerStatsView.name"
            icon="icons/listingView.gif"
            category="com.atlassw.tools.eclipse.checkstyle.category"
            class="net.sf.eclipsecs.stats.views.MarkerStatsView"
            id="net.sf.eclipsecs.stats.views.MarkerStatsView">
        </view>
        <view
            name="%GraphStatsView.name"
            icon="icons/graphView.gif"
            category="com.atlassw.tools.eclipse.checkstyle.category"
            class="net.sf.eclipsecs.stats.views.GraphStatsView"
            id="net.sf.eclipsecs.stats.views.GraphStatsView">
        </view>
    </extension>
    <extension
        point="org.eclipse.core.runtime.preferences">
        <initializer class="net.sf.eclipsecs.stats.PrefsInitializer"/>
    </extension>
    
</fragment>
